{
  "name": "react-number-card",
  "packageManager": "pnpm@7.22.0",
  "typings": "./dist/typings",
  "scripts": {
    "build": "redrun --series build:components build:parcel",
    "build:components": "redrun --parallel build:sass build:tsc",
    "build:sass": "node sass-compile.js",
    "build:tsc": "tsc -p .",
    "build:parcel": "parcel example/index.html --dist-dir=example-dist"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "parcel": "^2.8.2",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redrun": "^10.0.2",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "write": "^2.0.0"
  }
}
